{
  "name": "react-easy-animations",
  "description": "A React Component that provides regular animation for common object and some animation effects especially for typography like letters and multi-line text.",
  "keywords": ["react", "css", "animation", "animations", "text", "reveal"],
  "homepage": "https://prismosoft.github.io/react-easy-animations/",
  "version": "0.0.9",
  "author": "prismosoft",
  "license": "ISC",
  "main": "build/index.tsx",
  "files": ["build/*"],
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags",
    "predeploy-page": "cd example && npm install && npm run build",
    "deploy-page": "npm run predeploy-page && gh-pages -d example/dist",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "style-loader": "^1.1.2",
    "styled-components": "^5.3.6",
    "ts-loader": "^9.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "styled-components": ">= 4"
  },
  "dependencies": {
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8"
  },
  "repository": "github:prismosoft/react-easy-animations"
}
